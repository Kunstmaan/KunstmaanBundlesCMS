{% if node.node.deleted ==false %}
{% set nodemenuCurrent = nodemenu.current %}
<li id="{{ node.node.id }}" sequence="{{ node.node.getSequencenumber() }}" class="{% if node.active %}jstree-open{% endif %} {% if nodemenuCurrent is not null and nodemenuCurrent.id == node.id  %}active{% endif %} {% if not node.online %}offline{% endif %}" rel="page">
    <a href="{{ path('KunstmaanAdminNodeBundle_pages_edit', { 'id': node.id }) }}">{{ node.title }}</a>
    <ul>
        {% for node in node.children %}
            {% include 'KunstmaanAdminNodeBundle:Pages:rectreeview.html.twig' %}
        {% endfor %}
    </ul>
</li>
{% endif %}