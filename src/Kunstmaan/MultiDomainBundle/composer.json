{
  "name": "kunstmaan/multidomain-bundle",
  "type": "symfony-bundle",
  "description": "This bundle makes it possible to manage multiple websites from within KunstmaanCMS.",
  "keywords": [
    "kunstmaan",
    "navigation",
    "subsites",
    "multidomain"
  ],
  "homepage": "https://github.com/Kunstmaan/KunstmaanMultiDomainBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Kunstmaan CMS Team",
      "homepage": "https://kunstmaancms.be",
      "email": "cms@kunstmaan.be"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "kunstmaan/admin-bundle": "^5.9|^6.0",
    "kunstmaan/adminlist-bundle": "^5.9|^6.0"
  },
  "require-dev": {
    "matthiasnoback/symfony-config-test": "^4.2.1",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
    "symfony/phpunit-bridge": "^6.2",
    "phpunit/phpunit": "^9.5.5"
  },
  "autoload": {
    "psr-4": { "Kunstmaan\\MultiDomainBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-6.x": "6.1-dev"
    }
  }
}
