{
    "name": "kunstmaan/article-bundle",
    "type": "symfony-bundle",
    "description": "Add articles to your website. Use the KunstmaanGeneratorBundle to generate a new Overview/Detail article section for your website",
    "keywords": [ "kunstmaan", "cms", "article", "news"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanArticleBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^7.2",
        "kunstmaan/adminlist-bundle": "^5.2",
        "kunstmaan/pagepart-bundle": "^5.2"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/phpunit-bridge": "^5.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": { "Kunstmaan\\ArticleBundle\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.8-dev"
        }
    }
}
