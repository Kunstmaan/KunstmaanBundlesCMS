{
    "name": "kunstmaan/article-bundle",
    "type": "symfony-bundle",
    "description": "Add articles to your website. Use the KunstmaanGeneratorBundle to generate a new Overview/Detail article section for your website",
    "keywords": [ "kunstmaan", "cms", "article", "news"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanArticleBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "homepage": "https://kunstmaancms.be",
            "email": "cms@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^8.0",
        "kunstmaan/adminlist-bundle": "^6.3|^7.0",
        "kunstmaan/node-bundle": "^6.3|^7.0",
        "kunstmaan/pagepart-bundle": "^6.3|^7.0",
        "pagerfanta/core": "^2.4",
        "pagerfanta/twig": "^2.4"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "symfony/phpunit-bridge": "^6.1",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": { "Kunstmaan\\ArticleBundle\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-6.x": "6.1-dev"
        }
    }
}
