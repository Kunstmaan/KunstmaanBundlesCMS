{
    "name": "kunstmaan/search-bundle",
    "type": "symfony-bundle",
    "description": "The KunstmaanSearchBundle works with ElasticSearch and supports different search providers. The bundle currently supports Elastica as a provider. Add your own objects to index using a tagged service which implements the SearchConfigurationInterface",
    "keywords": [
        "kunstmaan",
        "search",
        "elasticsearch"
    ],
    "homepage": "https://github.com/Kunstmaan/KunstmaanSearchBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/console": "^3.4",
        "symfony/yaml": "^3.4",
        "ruflin/elastica": "^5.1|^6.0"
    },
    "suggest": {
        "kunstmaan/search-bundle": "^5.0 - to be put into require-dev when ready"
    },
    "autoload": {
        "psr-4": {
            "Kunstmaan\\SearchBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
