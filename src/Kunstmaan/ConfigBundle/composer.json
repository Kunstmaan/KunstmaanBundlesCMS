{
    "name": "kunstmaan/config-bundle",
    "type": "symfony-bundle",
    "description": "The Kunstmaan Config bundle supplies your project with custom configuration entities that can be managed from within KunstmaanCMS",
    "keywords": ["kunstmaan", "cms", "admin", "config"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanConfigBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "homepage": "https://kunstmaancms.be",
            "email": "cms@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^8.0",
        "kunstmaan/admin-bundle": "^6.3|^7.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "symfony/phpunit-bridge": "^6.1",
        "phpunit/phpunit": "^9.5.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Kunstmaan\\ConfigBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-6.x": "6.1-dev"
        }
    }
}
