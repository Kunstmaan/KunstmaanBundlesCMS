{
    "name": "kunstmaan/lead-generation-bundle",
    "type": "symfony-bundle",
    "description": "This bundle makes it easy to create lead generation popups for your website.",
    "keywords": [
        "kunstmaan",
        "lead generation",
        "leads",
        "popup",
        "modal"
    ],
    "homepage": "https://github.com/Kunstmaan/KunstmaanLeadGenerationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "homepage": "https://kunstmaancms.be",
            "email": "cms@kunstmaan.be"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "kunstmaan/adminlist-bundle": "^7.0",
        "symfony/deprecation-contracts": "^2.5|^3.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "symfony/phpunit-bridge": "^7.0",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Kunstmaan\\LeadGenerationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-6.x": "6.1-dev"
        }
    }
}
