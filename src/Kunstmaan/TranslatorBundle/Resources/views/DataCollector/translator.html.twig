{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span>
            <img height="28" alt="Translations" src="data:image/png;base64,"/>

            <span>Translations ({{ collector.totalTranslations }})</span>
        </span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Translations used</b>
            <span>{{ collector.totalTranslations }}</span>
        </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}


{% block menu %}
<span class="label">
    <span class="icon">
        <img height="30" alt="Translations" src="data:image/png;base64,"/>
    </span>
    <strong>Translations</strong>
</span>
{% endblock %}

{% block panel %}
    <h2>
        Translations ({{ collector.totalTranslations }})
    </h2>

    <table>
        <thead>
            <tr>
                <th>Domain</th>
                <th>Locale</th>
                <th>Keyword</th>
                <th>Text</th>
            </tr>
        </thead>
        <tbody>
            {% for translation in collector.translations %}
                <tr>
                    <td>{{ translation.domain }}</td>
                    <td>{{ translation.locale }}</td>
                    <td>{{ translation.keyword }}</td>
                    <td>{{ translation.text | truncate(40) }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}
