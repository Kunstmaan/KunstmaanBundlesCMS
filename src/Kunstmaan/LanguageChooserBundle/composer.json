{
    "name": "kunstmaan/languagechooser-bundle",
    "type": "symfony-bundle",
    "description": "The Kunstmaan Language chooser bundle supplies your project with basic but extendible language chooser ",
    "keywords": ["kunstmaan", "cms", "admin"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanLanguageChooserBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.3",
        "lunetics/locale-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Kunstmaan\\LanguageChooserBundle": "" }
    },
    "target-dir": "Kunstmaan/LanguageChooserBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
