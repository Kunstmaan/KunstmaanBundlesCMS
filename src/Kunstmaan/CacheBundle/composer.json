{
    "name": "kunstmaan/cache-bundle",
    "type": "symfony-bundle",
    "description": "The Kunstmaan Cache bundle supplies your project with the ability to ban certain pages from Varnish from within KunstmaanCMS",
    "keywords": ["kunstmaan", "cms", "admin", "varnish"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanCacheBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "https://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "kunstmaan/admin-bundle": "^5.9|^6.0",
        "kunstmaan/node-bundle": "^5.9|^6.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/phpunit-bridge": "^5.3",
        "phpunit/phpunit": "^8.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Kunstmaan\\CacheBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-6.x": "6.1-dev"
        }
    }
}
