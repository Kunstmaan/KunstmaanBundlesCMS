{
    "name": "kunstmaan/user-management-bundle",
    "type": "symfony-bundle",
    "description": "This bundle adds user, group & role settings in the backend.",
    "keywords": ["kunstmaan", "cms", "user", "group", "role", "admin"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanUserManagementBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "homepage": "https://kunstmaancms.be",
            "email": "cms@kunstmaan.be"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "kunstmaan/admin-bundle": "^6.3|^7.0",
        "kunstmaan/adminlist-bundle": "^6.3|^7.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "symfony/phpunit-bridge": "^6.1",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": { "Kunstmaan\\UserManagementBundle\\": "" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-6.x": "6.1-dev"
        }
    }
}
