{
    "name": "kunstmaan/media-bundle",
    "type": "symfony-bundle",
    "description": "To build your perfect website you probably need images, video's or maybe even a presentation too. The Kunstmaan Media Bundle handles all those media assets and centralizes them so you can find your content just the way you like it: fast and efficiently.",
    "keywords": ["kunstmaan", "cms", "media"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanMediaBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "homepage": "https://kunstmaancms.be",
            "email": "cms@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "gedmo/doctrine-extensions": "^2.4.34|^3.1.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "liip/imagine-bundle": "^2.4",
        "symfony/mime": "^4.4",
        "knplabs/knp-gaufrette-bundle": "~0.1",
        "kunstmaan/adminlist-bundle": "^5.9|^6.0",
        "pagerfanta/twig": "^2.4"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/phpunit-bridge": "^5.1",
        "phpunit/phpunit": "^8.5"
    },
    "conflict": {
        "doctrine/persistence": "<1.3"
    },
    "suggest": {
        "ext-imagick": "to support PDF preview images"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Kunstmaan\\MediaBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-6.0": "6.0-dev"
        }
    }
}
