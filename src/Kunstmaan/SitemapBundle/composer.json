{
    "name": "kunstmaan/sitemap-bundle",
    "type": "symfony-bundle",
    "description": "A sitemap bundle to generate a sitemap for your Kunstmaan bundles website based on its Nodes",
    "keywords": [ "kunstmaan", "cms", "sitemap"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanSitemapBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan CMS Team",
            "homepage": "https://kunstmaancms.be",
            "email": "cms@kunstmaan.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "kunstmaan/admin-bundle": "^6.1",
        "kunstmaan/node-bundle": "^5.9|^6.0",
        "kunstmaan/pagepart-bundle": "^5.9|^6.0",
        "doctrine/collections": "^1.6"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2.1",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "symfony/phpunit-bridge": "^6.2",
        "phpunit/phpunit": "^9.5.5"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Kunstmaan\\SitemapBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-6.x": "6.1-dev"
        }
    }
}
