{
    "name": "kunstmaan/node-search-bundle",
    "type": "symfony-bundle",
    "description": "This bundle uses the KunstmaanSearchBundle to search through Nodes from the KunstmaanNodeBundle",
    "keywords": [
        "kunstmaan",
        "node",
        "search",
        "elasticsearch"
    ],
    "homepage": "https://github.com/Kunstmaan/KunstmaanNodeSearchBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^8.0",
        "kunstmaan/admin-bundle": "^5.9|^6.0",
        "kunstmaan/pagepart-bundle": "^5.9|^6.0",
        "kunstmaan/node-bundle": "^5.9|^6.0",
        "kunstmaan/search-bundle": "^5.9|^6.0",
        "pagerfanta/core": "^2.4",
        "pagerfanta/twig": "^2.4",
        "ruflin/elastica": "^6.0|^7.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/phpunit-bridge": "^5.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": { "Kunstmaan\\NodeSearchBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.10-dev"
        }
    }
}
