{
    "name": "kunstmaan/bundles-cms",
    "type": "library",
    "description": "The Kunstmaan Bundles CMS is an advanced yet user-friendly content management system, based on the full stack Symfony2 framework combined with a whole host of community bundles. It provides a full featured, multi-language CMS system with an innovative page and form assembling process, versioning, workflow, translation and media managers and much more.",
    "keywords": ["cms"],
    "homepage": "http://bundles.kunstmaan.be",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "email": "bundles@kunstmaan.be"
        },
        {
            "name": "The Community",
            "homepage": "https://github.com/Kunstmaan/KunstmaanBundlesCMS/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.9",

        "symfony/symfony": "~2.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.3",
        "sensio/distribution-bundle": ">=2.3.0",
        "sensio/framework-extra-bundle": ">=2.3.0",
        "incenteev/composer-parameter-handler": "~2.0",

        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "knplabs/knp-menu-bundle": "~2.0",
        "guzzle/guzzle": "3.8.*",
        "white-october/pagerfanta-bundle": "1.0.*",
        "phpoffice/phpexcel": "1.8.*",
        "kunstmaan/google-api-custom": "~1.0.0",
        "ddeboer/data-import-bundle": "0.1.*",
        "gedmo/doctrine-extensions": "2.3.*",
        "lunetics/locale-bundle": "*",
        "doctrine/doctrine-fixtures-bundle": "~2.2.0",
        "stof/doctrine-extensions-bundle": "1.1.*@dev",
        "liip/imagine-bundle": "v0.20.2",
        "knplabs/knp-gaufrette-bundle": "0.1.*",
        "symfony-cmf/routing-bundle": "~1.1.1",
        "ruflin/elastica": "~1.0",
        "fpn/doctrine-extensions-taggable": "0.9.0",
        "sensio/generator-bundle": ">=2.3.0, <2.5.0"
    },
    "require-dev": {
        "behat/behat": "~2.5.0",
        "behat/mink": "*",
        "behat/symfony2-extension": "*",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-sahi-driver": "*",
        "phpunit/phpunit": "~4.4",
        "fzaninotto/faker": "~1.4.0",
        "nelmio/alice": "~1.6"
    },
    "replace": {
        "kunstmaan/admin-bundle": "self.version",
        "kunstmaan/adminlist-bundle": "self.version",
        "kunstmaan/article-bundle": "self.version",
        "kunstmaan/form-bundle": "self.version",
        "kunstmaan/media-bundle": "self.version",
        "kunstmaan/media-pagepart-bundle": "self.version",
        "kunstmaan/node-bundle": "self.version",
        "kunstmaan/node-search-bundle": "self.version",
        "kunstmaan/pagepart-bundle": "self.version",
        "kunstmaan/search-bundle": "self.version",
        "kunstmaan/seo-bundle": "self.version",
        "kunstmaan/sitemap-bundle": "self.version",
        "kunstmaan/utilities-bundle": "self.version",
        "kunstmaan/translator-bundle": "self.version",
        "kunstmaan/redirect-bundle": "self.version",
        "kunstmaan/user-management-bundle": "self.version",
        "kunstmaan/dashboard-bundle": "self.version",
        "kunstmaan/behat-bundle": "self.version",
        "kunstmaan/generator-bundle": "self.version",
        "kunstmaan/live-reload-bundle": "self.version",
        "kunstmaan/voting-bundle": "self.version",
        "kunstmaan/languagechooser-bundle": "self.version",
        "kunstmaan/tagging-bundle": "self.version"
    },
    "autoload": {
        "psr-4": {"Kunstmaan\\": "src/Kunstmaan/"}
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
